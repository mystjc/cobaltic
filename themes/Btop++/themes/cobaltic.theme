# All graphs and meters can be gradients
# For single color graphs leave "mid" and "end" variable empty.
# Use "start" and "end" variables for two color gradient
# Use "start", "mid" and "end" for three color gradient

# Main background, empty for terminal default, need to be empty if you want transparent background
theme[main_bg]="#242f3b"

# Main text color
theme[main_fg]="#b3c8e1"

# Title color for boxes
theme[title]="#76a6e4"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#76c4e0"

# Background color of selected item in processes box
theme[selected_bg]="#4586c4"

# Foreground color of selected item in processes box
theme[selected_fg]="#c2ddff"

# Color of inactive/disabled text
theme[inactive_fg]="#344a5e"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#b3c8e1"

# Cpu box outline color
theme[cpu_box]="#344a5e"

# Memory/disks box outline color
theme[mem_box]="#344a5e"

# Net up/down box outline color
theme[net_box]="#344a5e"

# Processes box outline color
theme[proc_box]="#344a5e"

# Box divider line and small boxes line color
theme[div_line]="#344a5e"

# Temperature graph colors
theme[temp_start]="#ace37e"
theme[temp_mid]="#eec36a"
theme[temp_end]="#ee6a65"

# CPU graph colors
theme[cpu_start]="#76c4e0"
theme[cpu_mid]="#76a6e4"
theme[cpu_end]="#d580e0"

# Mem/Disk free meter
theme[free_start]="#ee6a65"
theme[free_mid]="#eec36a"
theme[free_end]="#ace37e"

# Mem/Disk cached meter
theme[cached_start]="#ace37e"
theme[cached_mid]="#eec36a"
theme[cached_end]="#ee6a65"

# Mem/Disk available meter
theme[available_start]="#ee6a65"
theme[available_mid]="#eec36a"
theme[available_end]="#ace37e"

# Mem/Disk used meter
theme[used_start]="#ace37e"
theme[used_mid]="#eec36a"
theme[used_end]="#ee6a65"

# Download graph colors
theme[download_start]="#ace37e"
theme[download_mid]="#eec36a"
theme[download_end]="#ee6a65"

# Upload graph colors
theme[upload_start]="#ace37e"
theme[upload_mid]="#eec36a"
theme[upload_end]="#ee6a65"
