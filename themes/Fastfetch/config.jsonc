{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "padding": {
      "top": 3
    }
  },
  "general": {
    "multithreading": true
  },
  "display": {
    "separator": " • "
  },
  "modules": [
    "break",
    {
      "type": "custom",
      "format": "╭────────────────────────── Software Info ──────────────────────────╮"
    },
    {
      "type": "os",
      "format": "{3} {12}",
      "key": " ╭─ "
    },
    {
      "type": "kernel",
      "format": "{1} ({2})",
      "key": " ├─ "
    },
    {
      "type": "packages",
      "format": "{} (pacman)",
      "key": " ├─󰮯 "
    },
    {
      "type": "uptime",
      "key": " ├─ "
    },
    {
      "type": "wm",
      "key": " ├─ "
    },
    {
      "type": "shell",
      "format": "{3} ({4})",
      "key": " ├─󰞷 "
    },
    {
      "type": "terminal",
      "format": "{5} ({6})",
      "key": " ╰─ "
    },
    {
      "type": "custom",
      "format": "├────────────────────────── Hardware Info ──────────────────────────┤"
    },
    {
      "type": "host",
      "format": "{5} {1}{2}",
      "key": " ╭─󰟀 "
    },
    {
      "type": "cpu",
      "format": "{1}",
      "key": " ├─ "
    },
    {
      "type": "gpu",
      "format": "{1} {2} ({6})",
      "key": " ├─󱤓 "
    },
    {
      "type": "disk",
      "format": "{1} / {2} ({3}) - {10} ({9})",
      "key": " ├─ "
    },
    {
      "type": "memory",
      "key": " ╰─ "
    },
    {
      "type": "custom",
      "format": "╰───────────────────────────────────────────────────────────────────╯"
    },
    {
      "type": "custom",
      "format": "\t\t\t  {#red}󱓻 {#green}󱓻 {#yellow}󱓻 {#blue}󱓻 {#magenta}󱓻 {#cyan}󱓻   {#light_red}󱓻 {#light_green}󱓻 {#light_yellow}󱓻 {#light_blue}󱓻 {#light_magenta}󱓻 {#light_cyan}󱓻 "
    }
  ]
}
